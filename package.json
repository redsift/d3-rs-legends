{
  "name": "@redsift/d3-rs-legends",
  "version": "0.4.0",
  "description": "Generates legends for charts using D3v4.",
  "keywords": [
    "legend",
    "redsift",
    "d3",
    "chart"
  ],
  "homepage": "https://github.com/redsift/d3-rs-legends",
  "license": "MIT",
  "author": {
    "name": "Rahul Powar",
    "url": "https://medium.com/@rahulpowar"
  },
  "main": "distribution/d3-rs-legends.umd-es2015.js",
  "browser": "distribution/d3-rs-legends.umd-es2015.min.js",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-legends.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-legends -g d3-selection serve",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js bundle.config.js",
    "clean": "rm -rf dist",
    "pretest": "npm outdated || true && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint index.js src",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@redsift/d3-rs-svg": "^0.7.0",
    "@redsift/d3-rs-theme": "^0.6.0",
    "d3-format": "^1.2.2",
    "d3-selection": "^1.2.0"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^2.0.2",
    "@redsift/tape-reel": "~0.1.1",
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "eslint": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^3.0.2",
    "json2module": "~0.0.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^4.8.0"
  },
  "overrides": {
    "lodash": "4.17.21",
    "lodash.template": "4.5.0",
    "chokidar": "3.5.3"
  }
}
